                <artifactId>spring-boot-maven-plugin</artifactId>
                <version>2.5.6</version>
                <!--指定入口类-->
                <configuration>
                    <mainClass>com.php25.qiuqiu.admin.QiuQiuAdminApplication</mainClass>
                    <pullPolicy>IF_NOT_PRESENT</pullPolicy>
                </configuration>
